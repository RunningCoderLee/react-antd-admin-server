{
  "name": "swagger_petstore",
  "version": "1.0.0",
  "description": "This_is_a_sample_server_Petstore_server___You_can_find_out_more_about_____Swagger_at__httpswagger_io_httpswagger_io_or_on__irc_freenode_net_swagger_httpswagger_ioirc_______For_this_sample_you_can_use_the_api_key_special_key_to_test_the_authorization_____filters_",
  "license": "Unlicense",
  "main": "src/index.js",
  "scripts": {
    "pre-build-swagger": "rm -rf src/api/controllers src/api/swagger.yaml",
    "generate-swagger-code": "swagger-codegen generate -l nodejs-server -c src/api/config/configuration.json -i src/api/config/swagger.yaml -o src/api",
    "delete-useless-file": "mv src/api/api/* src/api && rm -rf src/api/README.md src/api/index.js src/api/.swagger-codegen-ignore src/api/package.json src/api/.swagger-codegen src/api/api",
    "build-swagger": "yarn pre-build-swagger && yarn generate-swagger-code && yarn delete-useless-file",
    "start": "export NODE_ENV=development && node ./bin/start.js",
    "test": "./node_modules/mocha/bin/mocha --recursive"
  },
  "browser": {
    "fs": false
  },
  "dependencies": {
    "body-parser": "1.17.1",
    "config": "1.26.2",
    "cookie-parser": "1.4.3",
    "cors": "^2.8.4",
    "debug": "2.6.3",
    "express": "4.15.2",
    "morgan": "1.8.1",
    "superagent": "3.5.2",
    "swagger-tools": "^0.10.1"
  },
  "devDependencies": {
    "eslint": "4.7.2",
    "eslint-config-airbnb-base": "12.0.0",
    "eslint-plugin-import": "2.7.0",
    "expect.js": "0.3.1",
    "mocha": "2.3.4",
    "sinon": "1.17.3"
  }
}
